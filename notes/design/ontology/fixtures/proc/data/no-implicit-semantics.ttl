@prefix wd: <https://paperhat.dev/ns/wd#> .

<urn:composition:proc:1> a wd:Composition ;
  wd:hasElement <urn:element:proc:1> .

<urn:element:proc:1> a wd:ElementInstance ;
  wd:style <urn:style:proc:1> ;
  wd:ownedBy <urn:composition:proc:1> .

<urn:style:proc:1> a wd:Style ;
  wd:opacity "1.0"^^<http://www.w3.org/2001/XMLSchema#decimal> ;
  wd:ownedBy <urn:composition:proc:1> .
