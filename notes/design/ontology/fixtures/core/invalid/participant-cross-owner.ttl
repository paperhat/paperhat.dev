@prefix wd:  <https://paperhat.dev/ns/wd#> .
@prefix ex:  <https://paperhat.dev/examples/core/invalid/participant-cross-owner#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:c1
  a wd:Composition ;
  wd:hasCanvas ex:canvas1 ;
  wd:hasElement ex:element1 ;
  wd:intent wd:Inform ;
  wd:expresses ex:p1 .

ex:c2
  a wd:Composition ;
  wd:hasCanvas ex:canvas2 ;
  wd:hasElement ex:element2 ;
  wd:intent wd:Inform .

ex:canvas1 a wd:Canvas ; wd:ownedBy ex:c1 .
ex:canvas2 a wd:Canvas ; wd:ownedBy ex:c2 .

ex:element1
  a wd:ElementInstance ;
  wd:ownedBy ex:c1 ;
  wd:instanceOf ex:type1 ;
  wd:frame ex:rect1 ;
  wd:style ex:style1 ;
  wd:semanticRole wd:Title ;
  wd:zIndex 1 .

ex:element2
  a wd:ElementInstance ;
  wd:ownedBy ex:c2 ;
  wd:instanceOf ex:type2 ;
  wd:frame ex:rect2 ;
  wd:style ex:style2 ;
  wd:semanticRole wd:BodyText ;
  wd:zIndex 1 .

ex:type1 a wd:ElementType .
ex:type2 a wd:ElementType .

ex:rect1
  a wd:Rect ;
  wd:ownedBy ex:c1 ;
  wd:x "0.0"^^xsd:decimal ;
  wd:y "0.0"^^xsd:decimal ;
  wd:w "20.0"^^xsd:decimal ;
  wd:h "10.0"^^xsd:decimal .

ex:rect2
  a wd:Rect ;
  wd:ownedBy ex:c2 ;
  wd:x "0.0"^^xsd:decimal ;
  wd:y "0.0"^^xsd:decimal ;
  wd:w "20.0"^^xsd:decimal ;
  wd:h "10.0"^^xsd:decimal .

ex:style1
  a wd:Style ;
  wd:ownedBy ex:c1 ;
  wd:opacity "1.0"^^xsd:decimal .

ex:style2
  a wd:Style ;
  wd:ownedBy ex:c2 ;
  wd:opacity "1.0"^^xsd:decimal .

ex:p1
  a wd:PrincipleStatement ;
  wd:ownedBy ex:c1 ;
  wd:principleType wd:Contrast ;
  wd:scope ex:c1 ;
  wd:participant ex:element2 .
