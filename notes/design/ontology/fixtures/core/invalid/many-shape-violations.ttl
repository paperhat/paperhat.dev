@prefix wd:  <https://paperhat.dev/ns/wd#> .
@prefix ex:  <https://paperhat.dev/examples/core/invalid/many-shape-violations#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:c1
  a wd:Composition ;
  wd:hasCanvas ex:canvas1 ;
  wd:hasElement ex:element1 ;
  wd:hasGrid ex:grid1 ;
  wd:intent wd:Inform ;
  wd:expresses ex:p1 .

ex:canvas1
  a wd:Canvas ;
  wd:ownedBy ex:c1 .

ex:element1
  a wd:ElementInstance ;
  wd:ownedBy ex:c1 ;
  wd:instanceOf ex:type1 ;
  wd:frame ex:rect1 ;
  wd:style ex:style1 ;
  wd:semanticRole wd:Title ;
  wd:zIndex 1 .

ex:type1 a wd:ElementType .

ex:rect1
  a wd:Rect ;
  wd:ownedBy ex:c1 ;
  wd:y "0.0"^^xsd:decimal ;
  wd:w "100.0"^^xsd:decimal ;
  wd:h "20.0"^^xsd:decimal .

ex:style1
  a wd:Style ;
  wd:ownedBy ex:c1 ;
  wd:opacity "1.2"^^xsd:decimal ;
  wd:fill ex:paint1 ;
  wd:stroke ex:stroke1 ;
  wd:typeStyle ex:typeStyle1 .

ex:paint1
  a wd:Paint ;
  wd:ownedBy ex:c1 ;
  wd:paintKind "solid" .

ex:stroke1
  a wd:Stroke ;
  wd:ownedBy ex:c1 ;
  wd:strokeWidth "-1.0"^^xsd:decimal .

ex:typeStyle1
  a wd:TypographicStyle ;
  wd:ownedBy ex:c1 ;
  wd:typeface ex:typeface1 ;
  wd:fontSize "0.0"^^xsd:decimal ;
  wd:fontWeight 0 .

ex:typeface1 a wd:Typeface .

ex:grid1
  a wd:GridSystem ;
  wd:ownedBy ex:c1 ;
  wd:rowCount 2 ;
  wd:hasUnit ex:u1 .

ex:u1
  a wd:GridUnit, wd:ColumnUnit, wd:RowUnit ;
  wd:ownedBy ex:c1 .

ex:p1
  a wd:PrincipleStatement ;
  wd:ownedBy ex:c1 ;
  wd:principleType wd:Contrast ;
  wd:participant ex:element1 .
