@prefix gd:  <https://paperhat.dev/ns/gd#> .
@prefix ex:  <https://paperhat.dev/examples/core/invalid/figureground-foreground-not-participant#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:c1
  a gd:Composition ;
  gd:hasCanvas ex:canvas1 ;
  gd:hasElement ex:element1, ex:element2 ;
  gd:hasRegion ex:region1 ;
  gd:intent gd:Inform ;
  gd:expresses ex:fg1 .

ex:canvas1
  a gd:Canvas ;
  gd:ownedBy ex:c1 .

ex:element1
  a gd:ElementInstance ;
  gd:ownedBy ex:c1 ;
  gd:instanceOf ex:type1 ;
  gd:frame ex:rect1 ;
  gd:style ex:style1 ;
  gd:semanticRole gd:Title ;
  gd:zIndex 2 .

ex:element2
  a gd:ElementInstance ;
  gd:ownedBy ex:c1 ;
  gd:instanceOf ex:type2 ;
  gd:frame ex:rect2 ;
  gd:style ex:style2 ;
  gd:semanticRole gd:BodyText ;
  gd:zIndex 1 .

ex:type1 a gd:ElementType .
ex:type2 a gd:ElementType .

ex:rect1
  a gd:Rect ;
  gd:ownedBy ex:c1 ;
  gd:x "0.0"^^xsd:decimal ;
  gd:y "0.0"^^xsd:decimal ;
  gd:w "40.0"^^xsd:decimal ;
  gd:h "20.0"^^xsd:decimal .

ex:rect2
  a gd:Rect ;
  gd:ownedBy ex:c1 ;
  gd:x "0.0"^^xsd:decimal ;
  gd:y "24.0"^^xsd:decimal ;
  gd:w "40.0"^^xsd:decimal ;
  gd:h "20.0"^^xsd:decimal .

ex:style1
  a gd:Style ;
  gd:ownedBy ex:c1 ;
  gd:opacity "1.0"^^xsd:decimal .

ex:style2
  a gd:Style ;
  gd:ownedBy ex:c1 ;
  gd:opacity "1.0"^^xsd:decimal .

ex:region1
  a gd:Region ;
  gd:ownedBy ex:c1 .

ex:fg1
  a gd:PrincipleStatement, gd:FigureGroundStatement ;
  gd:ownedBy ex:c1 ;
  gd:principleType gd:FigureGround ;
  gd:scope ex:c1 ;
  gd:participant ex:element2 ;
  gd:foreground ex:element1 ;
  gd:background ex:region1 .
