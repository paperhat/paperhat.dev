@prefix wd:  <https://paperhat.dev/ns/wd#> .
@prefix ex:  <https://paperhat.dev/examples/core/valid/full#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:c1
  a wd:Composition ;
  wd:hasCanvas ex:canvas1 ;
  wd:hasElement ex:titleElement, ex:bodyElement ;
  wd:hasGrid ex:grid1 ;
  wd:hasRegion ex:region1 ;
  wd:hasLayer ex:layer1 ;
  wd:intent wd:Inform ;
  wd:expresses ex:fg1 .

ex:canvas1
  a wd:Canvas ;
  wd:ownedBy ex:c1 .

ex:titleElement
  a wd:ElementInstance ;
  wd:ownedBy ex:c1 ;
  wd:instanceOf ex:titleType ;
  wd:frame ex:titleRect ;
  wd:style ex:titleStyle ;
  wd:semanticRole wd:Title ;
  wd:zIndex 10 ;
  wd:layer ex:layer1 .

ex:bodyElement
  a wd:ElementInstance ;
  wd:ownedBy ex:c1 ;
  wd:instanceOf ex:bodyType ;
  wd:frame ex:bodyRect ;
  wd:style ex:bodyStyle ;
  wd:semanticRole wd:BodyText ;
  wd:zIndex 5 .

ex:titleType a wd:ElementType .
ex:bodyType a wd:ElementType .

ex:titleRect
  a wd:Rect ;
  wd:ownedBy ex:c1 ;
  wd:x "0.0"^^xsd:decimal ;
  wd:y "0.0"^^xsd:decimal ;
  wd:w "100.0"^^xsd:decimal ;
  wd:h "20.0"^^xsd:decimal .

ex:bodyRect
  a wd:Rect ;
  wd:ownedBy ex:c1 ;
  wd:x "0.0"^^xsd:decimal ;
  wd:y "24.0"^^xsd:decimal ;
  wd:w "100.0"^^xsd:decimal ;
  wd:h "60.0"^^xsd:decimal .

ex:titleStyle
  a wd:Style ;
  wd:ownedBy ex:c1 ;
  wd:opacity "1.0"^^xsd:decimal ;
  wd:fill ex:paint1 ;
  wd:stroke ex:stroke1 ;
  wd:typeStyle ex:typeStyle1 .

ex:bodyStyle
  a wd:Style ;
  wd:ownedBy ex:c1 ;
  wd:opacity "0.95"^^xsd:decimal ;
  wd:fill ex:paint2 .

ex:paint1
  a wd:Paint ;
  wd:ownedBy ex:c1 ;
  wd:paintKind "solid" ;
  wd:paintValue "#000000" .

ex:paint2
  a wd:Paint ;
  wd:ownedBy ex:c1 ;
  wd:paintKind "solid" ;
  wd:paintValue "#222222" .

ex:stroke1
  a wd:Stroke ;
  wd:ownedBy ex:c1 ;
  wd:strokeWidth "1.0"^^xsd:decimal .

ex:typeStyle1
  a wd:TypographicStyle ;
  wd:ownedBy ex:c1 ;
  wd:typeface ex:typefaceSans ;
  wd:fontSize "16.0"^^xsd:decimal ;
  wd:fontWeight 600 ;
  wd:leading "20.0"^^xsd:decimal ;
  wd:tracking "0.0"^^xsd:decimal .

ex:typefaceSans a wd:Typeface .

ex:grid1
  a wd:GridSystem ;
  wd:ownedBy ex:c1 ;
  wd:columnCount 12 ;
  wd:rowCount 8 ;
  wd:hasUnit ex:gridColUnit, ex:gridRowUnit, ex:gridGutterUnit ;
  wd:baselineGrid ex:baseline1 .

ex:gridColUnit
  a wd:GridUnit, wd:ColumnUnit ;
  wd:ownedBy ex:c1 .

ex:gridRowUnit
  a wd:GridUnit, wd:RowUnit ;
  wd:ownedBy ex:c1 .

ex:gridGutterUnit
  a wd:GridUnit, wd:GutterUnit ;
  wd:ownedBy ex:c1 .

ex:baseline1
  a wd:BaselineGrid ;
  wd:ownedBy ex:c1 ;
  wd:baselineStep "8.0"^^xsd:decimal .

ex:region1
  a wd:Region ;
  wd:ownedBy ex:c1 .

ex:layer1
  a wd:Layer ;
  wd:ownedBy ex:c1 .

ex:fg1
  a wd:PrincipleStatement, wd:FigureGroundStatement ;
  wd:ownedBy ex:c1 ;
  wd:principleType wd:FigureGround ;
  wd:scope ex:c1 ;
  wd:participant ex:titleElement, ex:bodyElement ;
  wd:foreground ex:titleElement ;
  wd:background ex:region1 .
